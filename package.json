  "description": "navbar component for zonal portal",
  "main": "dist/index.js",
  "types": "dist/main.d.ts",
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "webpack-dev-server --port 3200 --mode development",
    "svg-to-jsx": "svgr -d src/assets/svg src/assets/icons",
    "prepublishOnly": "babel ./src --out-dir ./dist -s inline",
    "coverage": "react-scripts test --coverage --watchAll=false"
  },
  "files": [
    "dist/*"
  ],
